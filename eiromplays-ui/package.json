{
  "name": "eiromplays-ui",
  "private": false,
  "version": "0.1.1",
  "scripts": {
    "build": "run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "react": "^18.0.0",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24",
    "tsc-alias": "^1.6.7",
    "typed-scss-modules": "^6.3.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "description": "A UI library used for my some of my React projects",
  "repository": "https://github.com/Eiromplays/Eiromplays.UI",
  "author": "Eirik Sjøløkken",
  "license": "MIT"
}
