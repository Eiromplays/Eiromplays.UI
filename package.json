{
  "name": "eiromplays-ui",
  "private": false,
  "version": "1.0.6",
  "scripts": {
    "build": "run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc && tsc-alias",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "npm run prettier -- --list-different",
    "validate-and-build": "npm-run-all --parallel check-types check-format lint build",
    "validate": "npm-run-all --parallel check-types && lint-staged"
  },
  "dependencies": {
    "@tanstack/react-location": "^3.7.4",
    "@tanstack/react-location-devtools": "^3.4.4",
    "@tanstack/react-location-elements-to-routes": "^3.4.4",
    "react": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0",
    "react-query": "^3.38.1"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.6.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "clsx": "^1.1.1",
    "dompurify": "^2.3.6",
    "intersection-observer": "^0.12.0",
    "marked": "^4.0.14",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-image-crop": "^10.0.0",
    "react-select": "^5.3.1",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@headlessui/react": "^1.6.0",
    "@hookform/resolvers": "^2.8.8",
    "@tailwindcss/typography": "^0.5.2",
    "@tailwindcss/ui": "^0.7.2",
    "@types/dompurify": "^2.3.3",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.5",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.1",
    "dompurify": "^2.3.6",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "intersection-observer": "^0.12.0",
    "marked": "^4.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-image-crop": "^10.0.0",
    "react-select": "^5.3.1",
    "react-toastify": "^8.2.0",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24",
    "tsc-alias": "^1.6.7",
    "typed-scss-modules": "^6.3.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-dts": "^1.1.1",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-mkcert": "^1.6.0",
    "vite-tsconfig-paths": "^3.4.1",
    "zod": "^3.14.4"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./*": "./dist/*"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "description": "A UI library used for my some of my React projects",
  "repository": "https://github.com/Eiromplays/Eiromplays.UI",
  "author": "Eirik Sjøløkken",
  "license": "MIT"
