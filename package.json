{
  "name": "eiromplays-ui",
  "version": "1.6.9-2",
  "scripts": {
    "build": "run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc && tsc-alias",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "npm run prettier -- --list-different",
    "validate-and-build": "npm-run-all --parallel check-types check-format lint build",
    "validate": "npm-run-all --parallel check-types && lint-staged"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@tanstack/react-location": "^3.7.4",
    "@tanstack/react-location-devtools": "^3.4.4",
    "@tanstack/react-query": "^4.0.5",
    "@tanstack/react-query-devtools": "4.0.5",
    "react": "^18.1.0",
    "react-avatar-editor": "^13.0.0",
    "react-countup": "^6.2.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0",
    "react-toastify": "^9.0.2",
    "zustand": "^4.2.0"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.6.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "clsx": "^1.1.1",
    "dompurify": "^2.3.6",
    "intersection-observer": "^0.12.0",
    "marked": "^4.0.14",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-select": "^5.3.1",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@headlessui/react": "1.6.6",
    "@hookform/resolvers": "2.9.7",
    "@tailwindcss/typography": "0.5.4",
    "@tailwindcss/ui": "0.7.2",
    "@types/d3": "7.4.0",
    "@types/dompurify": "2.3.3",
    "@types/marked": "4.0.3",
    "@types/node": "16.11.6",
    "@types/react": "18.0.26",
    "@types/react-avatar-editor": "^13.0.0",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.7",
    "axios": "0.27.2",
    "clsx": "1.2.1",
    "dayjs": "1.11.4",
    "dompurify": "2.3.10",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "intersection-observer": "0.12.2",
    "marked": "4.0.18",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.33.1",
    "react-icons": "4.4.0",
    "react-select": "5.4.0",
    "recharts": "2.1.12",
    "sass": "1.53.0",
    "tailwindcss": "3.1.6",
    "tsc-alias": "1.7.0",
    "typed-scss-modules": "6.5.0",
    "typescript": "4.7.4",
    "vite": "2.9.14",
    "vite-plugin-dts": "1.4.0",
    "vite-tsconfig-paths": "3.5.0",
    "zod": "3.17.9"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./*": "./dist/*"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "description": "A UI library used for my some of my React projects",
  "repository": "https://github.com/Eiromplays/Eiromplays.UI",
  "author": "Eirik Sjøløkken",
  "license": "MIT"
